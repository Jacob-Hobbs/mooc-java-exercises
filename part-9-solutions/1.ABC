public class Main {

    public static void main(String[] args) {
        
        C c = new C();

        c.a();
        c.b();
        c.c();
        
    }
}

public class A {
    
    public void a() {
        System.out.println("A");
    }
    
}

public class B extends A {
    
    public void b() {
        System.out.println("B");
    }
    
}

public class C extends B {
    
    public void c() {
        System.out.println("C");
    }
    
}
